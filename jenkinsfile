pipeline {
    agent any 
    checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'essai', url: 'git@github.com:amouloken/caesar-cipher.git']]])
 
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }

        }
    }
